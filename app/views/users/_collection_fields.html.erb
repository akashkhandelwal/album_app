<div class="collection_field">
	<%= f.label :name, "Album Name" %><br />
	<%= f.text_field :name %><br />
	<%= f.check_box :_destroy %>
	<%= f.label :_destroy, "Remove Album" %><br />
	<br />
	<%= f.fields_for :photos do |f1| %>
	  <%= render 'photo_fields', f: f1 %>
	<% end %>
	<p><%= link_to_add_fields "Add Photo", f, :photos %></p>
</div>
